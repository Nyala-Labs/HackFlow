#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2/node_modules/vite/bin/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2/node_modules/vite/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2/node_modules/vite/bin/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2/node_modules/vite/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
