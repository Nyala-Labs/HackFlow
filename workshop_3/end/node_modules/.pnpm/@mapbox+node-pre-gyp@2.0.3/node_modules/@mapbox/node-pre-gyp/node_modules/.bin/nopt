#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/nopt@8.1.0/node_modules/nopt/bin/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/nopt@8.1.0/node_modules/nopt/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/nopt@8.1.0/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/nopt@8.1.0/node_modules/nopt/bin/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/nopt@8.1.0/node_modules/nopt/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/nopt@8.1.0/node_modules:/home/donewithwork/Desktop/web_dev_series/workshop_3/end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../nopt@8.1.0/node_modules/nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../../../../../../nopt@8.1.0/node_modules/nopt/bin/nopt.js" "$@"
fi
